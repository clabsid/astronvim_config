{
	"Make PHP public method": {
		"prefix": "pubf",
		"body": [
			"public function ${1}(${2})",
			"{",
			"  ${3}",
			"}${0}"
		],
		"description": "Make PHP public method"
	},
	"Make PHP construct": {
		"prefix": "_c",
		"body": [
			"public function __construct(${1})",
			"{",
			"  ${2}",
			"}${0}"
		],
		"description": "Make PHP construct"
	},
	"Make Laravel Route Get": {
